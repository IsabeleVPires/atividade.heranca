public class Main {
    public static void main(String[] args) {
        DesenhoRetangulo retSimples = new RetanguloSimples();
        DesenhoRetangulo retArredondado = new RetanguloArredondado();
        DesenhoRetangulo retDuplaLinha = new RetanguloDuplaLinha();
        DesenhoRetangulo retAsciiArt = new RetanguloAsciiArt();

        System.out.println("Ret창ngulo Simples:");
        retSimples.desenhar("Texto Simples");

        System.out.println("\nRet창ngulo Arredondado:");
        retArredondado.desenhar("Texto Arredondado");

        System.out.println("\nRet창ngulo de Linhas Duplas:");
        retDuplaLinha.desenhar("Texto Dupla Linha");

        System.out.println("\nRet창ngulo ASCII Art:");
        retAsciiArt.desenhar("Texto ASCII Art");
    }
}
